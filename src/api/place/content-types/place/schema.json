{
  "kind": "collectionType",
  "collectionName": "places",
  "info": {
    "singularName": "place",
    "pluralName": "places",
    "displayName": "Place",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid"
    },
    "metadata": {
      "type": "component",
      "component": "general.metadata",
      "repeatable": false
    },
    "tip": {
      "type": "customField",
      "customField": "global::multi-select",
      "enum": [
        "included",
        "optional",
        "required"
      ],
      "name": "Tip",
      "required": false
    },
    "category_places": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::category-place.category-place",
      "mappedBy": "places"
    },
    "general_info": {
      "type": "component",
      "component": "place.general-info",
      "repeatable": false
    },
    "appointment": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::appointment.appointment",
      "mappedBy": "place"
    },
    "quantity_sold": {
      "type": "integer",
      "required": true
    },
    "services": {
      "type": "component",
      "component": "place.services",
      "repeatable": true
    },
    "reviews": {
      "type": "component",
      "component": "place.review",
      "repeatable": true
    },
    "payment_methods": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::payment-method.payment-method"
    },
    "place_owner": {
      "type": "component",
      "component": "place.place-owner",
      "repeatable": false
    },
    "promotions": {
      "type": "component",
      "component": "promotion.promotion-rule",
      "repeatable": true
    }
  }
}
